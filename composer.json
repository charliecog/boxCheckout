{
    "name": "charliecog/boxCheckout",
    "description": "A Simple ecommerce solution using Slim v3",
    "keywords": ["ecommerce", "rest", "router", "psr7"],
    "homepage": "https://github.com/charliecog/boxCheckout",
    "license": "MIT",
    "authors": [
        {
            "name": "Charlie Coggs"
        }
    ],
    "require": {
        "php": ">=5.6",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "BoxCheckout\\": "src/classes/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
